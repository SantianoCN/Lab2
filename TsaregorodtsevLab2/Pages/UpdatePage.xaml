<Page x:Class="TsaregorodtsevLab2.Pages.UpdatePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:TsaregorodtsevLab2.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="UpdatePage">

    <Grid>
        <TabControl>
            <TabItem Header="Меню">
                <Frame x:Name="UpdateMenuFrame" NavigationUIVisibility="Hidden"/>
            </TabItem>

            <TabItem Header="Блюдо">
                <Frame x:Name="UpdateDishFrame" NavigationUIVisibility="Hidden"/>
            </TabItem>

            <TabItem Header="Тип блюда">
                <Frame x:Name="UpdateDishTypeFrame" NavigationUIVisibility="Hidden"/>
            </TabItem>
        </TabControl>
    </Grid>
</Page>
